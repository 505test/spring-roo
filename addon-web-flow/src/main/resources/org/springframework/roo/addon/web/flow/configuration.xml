<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
	<springWebFlow>
		<dependencies>
			<dependency org="org.springframework.webflow" name="spring-binding" rev="2.0.9.RELEASE" />
			<dependency org="org.springframework.webflow" name="spring-webflow" rev="2.0.9.RELEASE" />
			<dependency org="org.springframework.webflow" name="spring-js" rev="2.0.9.RELEASE" />
			<dependency org="org.springframework" name="spring-tx" rev="${spring.version}" />
			<dependency org="org.jboss.el" name="jboss-el" rev="2.0.1.GA" />
		</dependencies>
		<repositories>
			<repository>
				<id>JBoss Repo</id>
				<name>JBoss Repo</name>
				<url>http://repository.jboss.com/maven2</url>
			</repository>
		</repositories>
	</springWebFlow>
</configuration>